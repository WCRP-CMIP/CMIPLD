{
  "@context": {
    "@version": 1.1,
    "@vocab": "https://wcrp-cmip.org/ontology#",
    
    "id": "@id",
    "type": "@type",
    
    "wcrp": "https://wcrp-cmip.org/ontology#",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dcterms": "http://purl.org/dc/terms/",
    
    "validation-key": {
      "@id": "wcrp:validationKey",
      "@type": "xsd:string",
      "@required": true,
      "@priority": 10
    },
    
    "ui-label": {
      "@id": "rdfs:label", 
      "@type": "xsd:string",
      "@required": true,
      "@priority": 9
    },
    
    "description": {
      "@id": "dcterms:description",
      "@type": "xsd:string", 
      "@required": true,
      "@priority": 8
    },
    
    "title": {
      "@id": "dcterms:title",
      "@type": "xsd:string",
      "@priority": 7
    },
    
    "creator": {
      "@id": "dcterms:creator",
      "@type": "@id",
      "@priority": 6
    },
    
    "created": {
      "@id": "dcterms:created",
      "@type": "xsd:dateTime",
      "@priority": 5
    },
    
    "modified": {
      "@id": "dcterms:modified", 
      "@type": "xsd:dateTime",
      "@priority": 4
    },
    
    "version": {
      "@id": "schema:version",
      "@type": "xsd:string",
      "@priority": 3
    },
    
    "status": {
      "@id": "wcrp:status",
      "@type": "@vocab",
      "@priority": 2
    },
    
    "keywords": {
      "@id": "schema:keywords",
      "@type": "xsd:string",
      "@container": "@list"
    },
    
    "seeAlso": {
      "@id": "rdfs:seeAlso",
      "@type": "@id",
      "@container": "@set"
    },
    
    "related": {
      "@id": "dcterms:relation",
      "@type": "@id",
      "@container": "@set"
    },
    
    "belongsTo": {
      "@id": "wcrp:belongsTo",
      "@type": "@id"
    },
    
    "hasComponent": {
      "@id": "wcrp:hasComponent", 
      "@type": "@id",
      "@container": "@set"
    },
    
    "definedBy": {
      "@id": "rdfs:isDefinedBy",
      "@type": "@id"
    },
    
    "references": {
      "@id": "dcterms:references",
      "@type": "@id",
      "@container": "@set"
    },
    
    "license": {
      "@id": "dcterms:license",
      "@type": "@id"
    },
    
    "contact": {
      "@id": "schema:contactPoint",
      "@type": "@id"
    },
    
    "experiment": {
      "@id": "wcrp:experiment",
      "@type": "@id"
    },
    
    "variable": {
      "@id": "wcrp:variable",
      "@type": "@id"
    },
    
    "institution": {
      "@id": "wcrp:institution",
      "@type": "@id"
    },
    
    "model": {
      "@id": "wcrp:model",
      "@type": "@id"
    },
    
    "category": {
      "@id": "wcrp:category",
      "@type": "@vocab"
    },
    
    "theme": {
      "@id": "wcrp:theme",
      "@type": "@vocab",
      "@container": "@list"
    }
  }
}
